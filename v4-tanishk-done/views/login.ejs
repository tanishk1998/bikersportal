<%include ./partials/header%>
	
	<div class="login-container">
		<h1 class="login-title">Login</h1>
		<form method="POST" action="/login">
			<div class="username-container">
				<label for="username" class="username-label">My Username Is</label>
				<input type="text" class="login-username" name="username" placeholder="Username" autocomplete="off">
			</div>

			<div class="password-container">
				<label for="password" class="password-label">My Password Is</label>
				<input type="password" class="login-password" name="password" placeholder="Password" autocomplete="off">
			</div>
			<img src="/images/tan3.png" alt="bike" class="skull-image">
			<a href="/forget">forget password</a>
			<div class="form-group">
				<button class="btn btn-default submit-button">Submit</button>
			</div>
			
			
			
		</form>
	</div>
	
<%include ./partials/footer%>
