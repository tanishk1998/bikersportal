<%include ../partials/header %>
	<div class="container">
		<h1 style="text-align: center;">Create New Post</h1>
		<div style="width:30%; margin:25px auto;">
			<form method="POST" action="/bikes" enctype="multipart/form-data">
				<div class="form-group">
					<input type="text" class="form-control" name="rtoNo" placeholder="registration number">
				</div>
				<div class="form-group">
					<input type="file" class="form-control" name="image" placeholder="image url">
				</div>
				<div class="form-group">
					<input type="text" name="description" placeholder="Description" style="height:90px; width:100%;">
				</div>
				<div class="form-group">
					<input type="text" name="model" placeholder="model">
				</div>
				<div class="form-group">
					<button class="btn btn-lg btn-primary btn-block">SUBMIT</button>
				</div>	
			</form>
			<a href="/bikes">GO BACK</a>
		</div>
		
		
	</div>
<%include ../partials/footer %>
